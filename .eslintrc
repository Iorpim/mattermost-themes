{
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "extends": [
      "./node_modules/eslint-plugin-mattermost/configs/.eslintrc.json",
      "./node_modules/eslint-plugin-mattermost/configs/.eslintrc-react.json"
    ],
    "rules": {
      "header/header": [0],
      "no-process-env": [0],
      "no-console": [0],
      "no-empty-function": [0],
      "no-case-declarations": [0],
      "mattermost/no-dispatch-getstate": [0]
    }
}